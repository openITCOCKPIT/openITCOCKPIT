{
    "name": "openitcockpit/mattermost-module",
    "description": "MattermostModule plugin for openITCOCKPIT",
    "type": "cakephp-plugin",
    "license": "Dual licensed",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.5 || ^11.1.3 || ^12.1"
    },
    "autoload": {
        "psr-4": {
            "MattermostModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MattermostModule\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}

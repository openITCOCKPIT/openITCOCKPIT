{
    "name": "openitcockpit/statusengine3-module",
    "description": "Statusengine3Module plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.5 || ^11.1.3 || ^12.1"
    },
    "autoload": {
        "psr-4": {
            "Statusengine3Module\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Statusengine3Module\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
